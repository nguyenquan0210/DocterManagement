                if (ProvinceId != "") {
                    $.ajax({
                        type: 'GET',
                        url: '/Login/GetDistrict?ProvinceId= + ' + ProvinceId, 
                        success: function (result) {
                            var s = '<option value="">- Quận Huyện -</option>';
                            for (var i = 0; i < result.length; i++) {
                                s += '<option value="' + result[i].value +'">' + result[i].text + '</option>';
                            }
                            $('#DistrictId').html(s);
                        }
                    })
                }else{
                    $('#DistrictId').html('<option value="" >- uận Huyện -</option>');
                }
            })
             $("#DistrictId").change(function () {
