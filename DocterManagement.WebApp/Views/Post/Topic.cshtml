                                    </picture>
                                    <noscript>
                                        <picture class="card-image img-fluid wp-post-image" loading="lazy">
                                            <source type="image/webp"
                                                srcset="@(Configuration["BaseAddress"] + "/img/" + item.Image) 1200w, @(Configuration["BaseAddress"] + "/img/" + item.Image) 300w, @(Configuration["BaseAddress"] + "/img/" + item.Image) 1024w, @(Configuration["BaseAddress"] + "/img/" + item.Image) 768w"
