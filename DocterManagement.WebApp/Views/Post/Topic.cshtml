                                                src="@(Configuration["BaseAddress"] + "/img/" + item.Image)"
                                                alt="" loading="lazy"
                                                srcset="@(Configuration["BaseAddress"] + "/img/" + item.Image) 1200w, @(Configuration["BaseAddress"] + "/img/" + item.Image) 300w, @(Configuration["BaseAddress"] + "/img/" + item.Image), @(Configuration["BaseAddress"] + "/img/" + item.Image) 768w"
                                                sizes="(max-width: 1200px) 100vw, 1200px" />
                                        </picture>
                                    </noscript>
                                </a>
                                <div class="block-card-body">
                                    <a class="post-title line-clamp-2 font-serif font-bold"
                                       asp-action="DetailtPost" asp-controller="Post" asp-route-id="@item.Id"
                                        data-wpel-link="internal">@item.Title</a>

                                    <div class="content-author">
                                        @{
                                            var m = (int) (DateTime.Now - item.CreatedAt).TotalMinutes;
                                            var text = m > (60*24*30*12) ? (item.CreatedAt.Day+" Thg"+item.CreatedAt.Month+", "+item.CreatedAt.Year) :m > (60*24*30) ? (m / (60*24*30)) + " tháng trước" :m > (60*24) ? (m / (60*24)) + " ngày trước" : m > 60 ? (m / 60) + " giờ trước" : "";
                                        }
                                        <div class="article-author"><a class="url fn n"
                                                asp-action="Index" asp-controller="Doctor" asp-route-id="@item.Doctors.UserId"
                                                data-wpel-link="internal"><span class="author vcard">@(item.Doctors.LastName+" "+item.Doctors.FirstName)</span></a></div>
                                                <time class="published-date">Ngày đăng:  @text </time>
                                    </div>
                                </div>
                            </article>
                            
                        }
