@using DoctorManagement.Data.Enums
@using DoctorManagement.ViewModels.Catalog.MasterData
@using DoctorManagement.ViewModels.Catalog.Post
@using DoctorManagement.ViewModels.Common;
@model PagedResult<DoctorManagement.ViewModels.Catalog.Post.PostVm>
@{
    ViewData["Title"] = "Topic";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var posts = (List<PostVm>)ViewBag.PostCategory;
    var topic_ = (MainMenuVm)ViewBag.Topic;
    var menus = (List<MainMenuVm>)ViewBag.Menus;
